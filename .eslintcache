lines?country=id&apiKey=d4eef434bb2848cca69d497ef7eb2b42';\n\nfunction NewsFeed() {\n  const [news, setNews] = useState(defaultNews);\n  const [page, setPage] = useState(1);\n  const [isLoading, setLoading] = useState(false);\n  const [isError, setError] = useState(false);\n  const [isRefresh, setRefresh] = useState(false);\n\n  const handleRefresh = () => {\n    setNews(defaultNews);\n    setPage(1);\n    setLoading(false);\n    setRefresh(false);\n  };\n  \n  useEffect(() => {\n    const fetchData = async () => {\n      setLoading(true)\n\n      // error handling\n      try {\n        let respon = await fetch(`${endpoint}&page=${page}`)\n        let result = await respon.json();\n\n        setNews(current => {\n          return (\n            {\n              ...result,\n              articles: [...current.articles, ...result.articles],\n              totalResult: result.totalResults,\n              status: result.status\n            }\n          )\n        });\n        \n        if (result.status != 'ok') throw new Error('error');\n      } catch (error) {\n        setError(true);\n      };\n\n      setLoading(false);\n    };\n\n    fetchData();\n  }, [page, isRefresh]);\n\n  return (\n    <div className=\"container\">\n      <h3> Top Headline News</h3>\n      {isLoading && <p> Loading ...</p>}\n      {isError && <p> Mohon maaf error !</p>}\n      <ol>\n        {news.articles.map((r, i) => (\n          <li key={i}>{r.title}</li>\n        ))}\n      </ol>\n      {\n        news.articles.length < parseInt(news.totalResult) \n        ? ( <button \n              disabled={isLoading}\n              onClick={ () => setPage(c => c + 1)}\n            >Load More</button> ) \n        : null\n      }\n      \n      {' '}<button onClick={handleRefresh} disabled={isLoading}> Refresh</button>\n    </div>\n  )\n}\n\nexport default NewsFeed;","/Users/bibur/Develop/riset/react-pertama/src/Views/day2/index.js",[],"/Users/bibur/Develop/riset/react-pertama/src/Views/day4/index.js",["